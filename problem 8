// Jean Piolo C. Ong, Experiment 2, Problem 8
#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
    int n;

    cout << "Enter a number: ";
    cin >> n;
    cout<< "\n\nMultiplication Table";

    for (int i = 1; i <= 10; ++i) {
        cout << "\n"<< n << " * " << i << " = " << n * i << endl;
    }
    
    return 0;
}
