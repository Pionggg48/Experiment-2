// Jean Piolo C. Ong, Experiment 2, Problem 9
#include <iostream>
using namespace std;

int main()
{
    int a,b;
    cout<<"Number of Rows: ";
    cin>>a;
    cout<<"Number of Columns: ";
    cin>>b;
    
    for (int c = 1; c <= a; c++)
    {
        for (int d = 1; d <= b; d++)
        {
            cout << "*";
        }
        cout<<endl;
    }
    return 0;
}
